generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    String    @id @default(uuid())
  name  String
  email String @unique
  age   Int
  createdAt DateTime @default(now())
  updateAt DateTime @default(now())
}

model Password {
  id String @id @default(uuid())
  accountEmail String @unique
  alias String
  value String
  description String?
  createdAt DateTime @default(now())
  updateAt DateTime @default(now())
}